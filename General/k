execommand(client,kubectl create namespace monitoring)
execommand(client,kubectl create -f clusterRole.yaml)
execommand(client,kubectl create -f config-map.yaml)
execommand(client,kubectl create  -f prometheus-deployment.yaml)
execommand(client,kubectl get deployments --namespace=monitoring)
execommand(client,kubectl get pods --namespace=monitoring)
execommand(client,kubectl create -f prometheus-service.yaml --namespace=monitoring)
execommand(client,kubectl create -f grafana-datasource-config.yaml)
execommand(client,kubectl create -f deployment.yaml)
execommand(client,kubectl create -f service.yaml)